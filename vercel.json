{
  "version": 2,
  "buildCommand": "npm run build:css",
  "builds": [
    { "src": "api/contact.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" },
    { "src": "robots.txt", "use": "@vercel/static" },
    { "src": "sitemap.xml", "use": "@vercel/static" },
    { "src": "CNAME", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact.js" }
  ]
}
